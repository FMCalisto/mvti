<script type="text/javascript">
	(function() {
    var rotator = document.getElementById('lightbox1');  
    var imageDir = '../images/lightboxw/2015/';                          
    var delayInSeconds = 5;                            
    // list image names
    var images = ['lightbox1_wacom_cst2.jpg', 'lightbox1_elgato_hd60.jpg', 'lightbox1_seagate.jpg', 'lightbox1_mykronoz.jpg',];
	 
	    // don't change below this line
    var num = 0;
    var changeImage = function() {
        var len = images.length;
	        rotator.src = imageDir + images[num++];
	        if (num == len) {
	            num = 0;
	        }
	    };
	    setInterval(changeImage, delayInSeconds * 1000);
	})();
